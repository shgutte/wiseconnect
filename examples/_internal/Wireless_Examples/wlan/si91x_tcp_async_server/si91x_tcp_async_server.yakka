name: Si91x TCP Async Server

sources:
  - main.c
  - app.c

includes:
  global:
    - .

clang-format: true

defines:
  global:
    - RSI_DEBUG_PRINTS

requires:
  components:
    - si91x_debug
    - wifi
    - sl_si91x_asynchronous_socket
    - network_manager
    - freertos
    - clang-format

flags:
  c:
    global:
      - -Wall 
      - -Werror
      - -u _printf_float