name: Wlan Throughput IPv6

sources:
  - main.c
  - app.c

clang-format: true

defines:
  global:
    - RSI_DEBUG_PRINTS

requires:
  features:
    - ipv6
  components:
    - si91x_debug
    - wifi
    - network_manager
    - bsd_socket
    - freertos
    - clang-format
    - resources

flags:
  ld:
    global:
      - -u _printf_float
  c:
    global:
      - -Wall
      - -Werror 
