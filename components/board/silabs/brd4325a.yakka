name: BRD4325A

includes:
  global:
    - inc
    - config/brd4325a

sources:
  - src/rsi_board.c

defines:
  global:
    - SI917_RADIO_BOARD

requires:
  features:
    - wifi_internal_bus
  components:
    - sl_si91x_mcu
    
flags:
  ld:
    global:
      - -Wl,--defsym=SI91X_SOC_ROM_START_ADDRESS=0x8212000
      - -Wl,--defsym=SI91X_SOC_RAM_START_ADDRESS=0xc

clang-format: true

silicon_labs:
  device:
    flash_addr: 136388608
    ram_size: 194560
